    },
    {
      "type": "git",
      "url": "https://github.com/proklung/bitrix.tools.pack.bundle"
    }
  ],

  "require": {
    "proklung/bitrix-core-symfony": "^1.0",
    "proklung/core-framework-extension-bundle": "^1.0",
    "proklung/core-argument-resolvers-bundle": "^1.0",
    "arrilot/bitrix-migrations": "^2.2",
    "arrilot/bitrix-models": "dev-master",
    "arrilot/bitrix-hermitage": "^1",
    "maximaster/tools.twig": "^1.1",
    "bitrix-expert/bbc": "^1.0",
    "illuminate/support": "^6.0",
    "illuminate/pagination": "^6.0",
    "illuminate/database": "^6.0",
    "illuminate/container": "^6.0",
    "vlucas/phpdotenv": "3.*",
    "bitrix-expert/monolog-adapter": "^1.0",
    "mmucklo/krumo": "^0.6.0",
    "symfony/dotenv": "^5.2",
    "symfony/mailer": "^5.2",
    "symfony/google-mailer": "^5.2",
    "symfony/proxy-manager-bridge": "4.*",
    "symfony/monolog-bundle": "^3.6",
    "league/flysystem": "^1.0",
